$TTL    60
@	IN	SOA	home.dmarby.se. hostmaster.dmarby.se. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@         IN      NS      {{ ns_hostname }}.
{{ ns_ip_reverse }}   IN      PTR     {{ ns_hostname }}.home.dmarby.se.
50.0.100  IN      PTR     vcenter.home.dmarby.se.

10.20.100 IN PTR storage.home.dmarby.se.
