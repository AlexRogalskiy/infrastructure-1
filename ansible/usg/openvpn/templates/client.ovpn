client
float
dev tun
remote {{ vpn_hostname }} 1194 udp
auth-user-pass
resolv-retry infinite
nobind
persist-key
persist-tun
cipher AES-256-CBC
comp-lzo
verb 5
<ca>
{{ ca_cert.content|b64decode }}
</ca>

<cert>
{{ client_cert.content|b64decode }}
</cert>

<key>
{{ client_key.content|b64decode }}
</key>
